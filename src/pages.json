		"list": [{
			"pagePath": "pages/home/index",
			"iconPath": "static/home-unactive.png",
			"selectedIconPath": "static/home-active.png",
			"text": "首页"
		},
		
		{
			"pagePath": "pages/mine/index",
			"iconPath": "static/mine-unactive.png",
			"selectedIconPath": "static/mine-active.png",
			"text": "个人中心"
		}]
	}
